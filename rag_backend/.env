# Environment variables for the RAG Backend

# Large Language Model (LLM) API Key
# Replace with your actual LLM API key (e.g., Anthropic API key, OpenAI API key)
LLM_API_KEY="your_llm_api_key_here"
LLM_MODEL_NAME="gemini-pro" # Or other model name as per your choice and free-tier access

# Qdrant Vector Database
# QDRANT_URL="http://localhost:6333" # For local Qdrant instance
# QDRANT_API_KEY="your_qdrant_api_key_here" # If using a hosted Qdrant service
QDRANT_HOST="localhost"
QDRANT_PORT="6333"

# Neon Serverless Postgres Database
# Replace with your actual Neon connection string
DATABASE_URL="postgresql://user:password@host:port/database"
